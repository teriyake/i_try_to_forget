{
    "manifest_version": 3,
    "name": "i try to forget",
    "version": "1.0.0",
    "description": "Trying to forget",
    "permissions": ["storage", "scripting", "alarms"],
    "host_permissions": ["<all_urls>"],
    "background": { "service_worker": "background.js" },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css": ["content.css"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["cursor_eraser_32x32.svg"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "48": "icon-48.png",
        "128": "icon-128.png"
    }
}
